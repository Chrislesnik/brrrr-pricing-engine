{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.ts",
    "css": "src/app/globals.css",
    "baseColor": "slate",
    "cssVariables": true,
    "prefix": ""
  },
  "iconLibrary": "lucide",
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils",
    "ui": "@/components/ui",
    "lib": "@/lib",
    "hooks": "@/hooks"
  },
  "registries": {
    "@lytenyte": "https://www.1771technologies.com/r/{name}.json",
    "@ss-blocks": "https://shadcnblocks.com/r/{name}.json",
    "@diceui": "https://diceui.com/r/{name}.json",
    "@shadcnstudio": "https://shadcnstudio.com/r/blocks/{name}.json",
    "@shadcndesign": {
      "url": "https://www.shadcndesign.com/api/registry/{name}",
      "headers": {
        "X-License-Key": "${SHADCNDESIGN_LICENSE_KEY}"
      }
    },
    "@shadcnexamples": "https://shadcnexamples.com/r/{name}.json"    
  }
}
